}
.custom-form .form-control {
	display: block;
	width: 100%;
	height: 38px;
	padding: 6px 12px;
	font-size: 14px;
	line-height: 1.42857143;
	color: #231F20;
	background-color: #FFFFFF;
	background-image: none;
	border: 1px solid #C2C2C2;
	border-radius: 3px;
	-webkit-box-shadow: none;
	box-shadow: none;
	-webkit-transition: none;
	-o-transition: none;
	transition: none;
}
.custom-form  label {
	display: inline-block;
	max-width: 100%;
	margin-bottom: 5px;
	font-weight: 400;
	color: #333;
}
.form-group.btnisnide.text-center {
	text-align: left;
	margin: 15px 0 0;
}
.custom-form label.error {
	color: red;
	font-size: 12px;
	line-height: 12px;
}
.fgot{
