.thim-login.form-submission-register.custom-form{
	padding: 40px 40px 40px;
	border: 1px solid #F2F2F2;
	background: #fdfdfd;
}
.custom-form .sc_heading.text-center h2 {
	margin: 0 0 30px !important;
