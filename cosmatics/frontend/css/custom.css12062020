	float: left;
	left: -25px;
}
.active-screen .number::after {
	content: url("../images/right-arrow.png");
	position: relative;
	left: 85px;
	top: -46px;
}
.active-screen .number.active a {
	color: #fff;
}
.active-screen .number a {
